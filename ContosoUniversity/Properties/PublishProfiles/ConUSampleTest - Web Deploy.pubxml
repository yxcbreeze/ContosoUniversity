  <ItemGroup>
    <MSDeployParameterValue Include="SchoolContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:conusampletest.database.windows.net,1433;Initial Catalog=ConUSample;User Id=yxcadmin@conusampletest.database.windows.net;Password=hello123456!;</ParameterValue>
