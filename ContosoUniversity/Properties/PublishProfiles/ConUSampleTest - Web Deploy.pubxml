    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="SchoolContext" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:conusampletest.database.windows.net,1433;Initial Catalog=ConUSample;User ID=yxcadmin@conusampletest.database.windows.net;Password=hello123456!" Name="Data Source=tcp:conusampletest.database.windows.net,1433;Initial Catalog=ConUSample;User Id=yxcadmin@conusampletest.database.windows.net;Password=hello123456!;" />
